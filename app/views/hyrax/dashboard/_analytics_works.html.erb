<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><%= t('.header') %></h3>
      </div>
      <div class="panel-body">
        <table id="analytics-works" class="table table-striped">
          <thead>
          <tr>
            <th><%= t('.title') %></th>
            <th><%= t('.deposited') %></th>
            <th><%= t('.views') %></th>
            <th><%= t('.work_type') %></th>
            <th><%= t('.visibility') %></th>
          </tr>
          </thead>
          <tbody>
          <% @work_rows.each do |row| %>
            <tr>
              <td><%= row.work_name %></td>
              <td><%= row.updated %></td>
              <td><%= row.work_views %></td>
              <td><%= row.work_type %></td>
              <td><%= row.visibility %></td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>